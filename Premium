-- Function to decode hex-encoded strings
local function hexDecode(s)
    return (s:gsub('..', function(cc)
        return string.char(tonumber(cc, 16))
    end))
end

-- Function to decode URL-encoded strings
local function urlDecode(s)
    return (s:gsub('%%(%x%x)', function(h)
        return string.char(tonumber(h, 16))
    end))
end

-- Function to get the original string from hex encoding
local function getStr(s)
    return loadstring("return '" .. hexDecode(s) .. "'")()
end

-- Function to convert encoded string to number
local function hexDecodeToNumber(s)
    return tonumber(hexDecode(s), 10)
end

-- Obfuscated whitelist numbers
local wl = {
    [hexDecodeToNumber(getStr('37303534383032303833'))] = true, -- 7054982083
    [hexDecodeToNumber(getStr('37303734343832353633'))] = true, -- 7074482563
    [hexDecodeToNumber(getStr('37303433323035343138'))] = true, -- 7043205418
}

-- Function to check if a player is whitelisted
local function checkWl(p)
    return wl[p.UserId]
end

if checkWl(game.Players.LocalPlayer) then

    -- Functions to encode and decode strings
    local function encodeStr(s)
        return (s:gsub('.', function(c)
            return string.format('%02X', string.byte(c))
        end))
    end

    local function getEncodedStr(s)
        return hexDecode(encodeStr(s))
    end

    -- Create GUI elements
    local a = Instance.new(getStr('53637265656E477569')) -- ScreenGui
    local b = Instance.new(getStr('4672616D65')) -- Frame
    local c = Instance.new(getStr('546578744C6162656C')) -- TextLabel
    local d = Instance.new(getStr('54657874426F78')) -- TextBox
    local e = Instance.new(getStr('54657874547574626F6E')) -- TextButton
    local f = Instance.new(getStr('546578744C6162656C')) -- TextLabel
    local g = Instance.new(getStr('546578744C6162656C')) -- TextLabel
    local h = Instance.new(getStr('546578744C6162656C')) -- TextLabel
