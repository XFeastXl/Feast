    e.TextColor3 = Color3.new(1, 1, 1)
    e.Text = getStr('5375626D6974')
    e.Size = UDim2.new(0, 100, 0, 30)
    e.Position = UDim2.new(0, 150, 0, 100)
    e.BackgroundColor3 = Color3.new(0, 1, 0)
    e.BackgroundTransparency = 0.5

    f.Font = Enum.Font.SourceSans
    f.FontSize = Enum.FontSize.Size18
    f.TextColor3 = Color3.new(1, 1, 1)
    f.Text = getStr('4A6F696E206F757220446973636F72643A2068747470733A2F2F646973636F72642E67672F4A425834473874715444')
    f.Size = UDim2.new(0, 200, 0, 20)
