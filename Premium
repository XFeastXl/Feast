    i.Image = getEncodedStr('687474703A2F2F7777772E726F626C6F782E636F6D2F61737365742F3F696420353535393339313231') -- Image URL
    i.Size = UDim2.new(0, 400, 0, 250)
    i.Position = UDim2.new(0, 0, 0, 0)
